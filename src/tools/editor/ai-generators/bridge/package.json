{
  "name": "nl-to-graph-bridge",
  "version": "1.0.0",
  "description": "Convert natural language descriptions into structured graph nodes using AI",
  "main": "dist/nl-to-graph-bridge.js",
  "types": "dist/nl-to-graph-bridge.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "example": "ts-node examples.ts",
    "start": "ts-node index.ts"
  },
  "keywords": [
    "graph",
    "ai",
    "nlp",
    "natural-language",
    "workflow",
    "diagram",
    "flowchart",
    "claude",
    "openai",
    "visualization"
  ],
  "author": "Nikla Lukic",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
